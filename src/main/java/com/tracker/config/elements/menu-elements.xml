<elements>

    <menu-element name="users" title="header.buttons.searchInbox" searchColumns="firstName,lastName,birthDay"  type="header" enableForRoles="ADMIN,USER">

        <sub-element name="searchUsers" title="header.buttons.searchUsers"  type="subElement" searchColumns="firstName,lastName,birthDay"
                     searchParams="{'searchType' : 'user'}"
                     enableForRoles="ADMIN,USER">
            <searcher type="text" name="firstName" title="header.buttons.firstName"></searcher>
            <searcher type="text" name="lastName" title="header.buttons.lastName"></searcher>
        </sub-element>
        <!-- searchParams="{'searchType' : 'user','searchData' : {'firstName' : 'admin'} }" -->

        <sub-element name="searchMyTasks" title="header.buttons.searchMyTasks"  type="subElement" searchColumns="issueName,issueDescription"
                     searchParams="{'searchType' : 'issue', 'searchMyTasks' : 'true'}"
                     enableForRoles="ADMIN,USER">
            <searcher type="text" name="issueName" title="issue.card.issueName"></searcher>
            <searcher type="text" name="issueDescription" title="issue.card.issueDescription"></searcher>
        </sub-element>


    </menu-element>

    <menu-element name="searchNews" title="header.buttons.searchNews" searchColumns="issueName,issueDescription" type="filter" enableForRoles="ADMIN,USER" searchParams="{'searchType' : 'issue'}">
        <searcher type="text" name="issueName" title="issue.card.issueName"></searcher>
        <searcher type="text" name="issueDescription" title="issue.card.issueDescription"></searcher>
    </menu-element>

    <menu-element name="test" title="header.buttons.test"  type="header" enableForRoles="ADMIN,USER">
        <sub-element name="searchUsers2" title="header.buttons.searchUsers2"  type="subElement"  searchColumns="firstName,lastName,birthDay"  enableForRoles="ADMIN,USER">
            <searcher type="text" name="firstName" title="header.buttons.firstName"></searcher>
            <searcher type="text" name="lastName" title="header.buttons.lastName"></searcher>
        </sub-element>

    </menu-element>

</elements>